<controls:MetroWindow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
    xmlns:Custom="http://metro.mahapps.com/winfx/xaml/shared" x:Class="ZanoFineTuning.AppWindow"
    Title="ZanoFineTuning"
    Height="720"
    Width="1280" 
    WindowStartupLocation="CenterScreen" 
    BorderThickness="2"
    BorderBrush="{DynamicResource Z-Brush-Gold}"
    NonActiveBorderBrush="{DynamicResource Z-Brush-Dark}"
    WindowTitleBrush="{DynamicResource Z-Brush-Gold}"
    NonActiveWindowTitleBrush="{DynamicResource Z-Brush-Dark}"
    TitleForeground="{DynamicResource Z-Brush-Black}"
    Background="{DynamicResource Z-Brush-Dark}"
    TitleCaps="True"
    Loaded="OnLoaded" Icon="/ZanoFineTuning;component/Icon.ico" ShowIconOnTitleBar="False" NonActiveGlowBrush="{DynamicResource Z-Brush-Gold}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Frame Grid.Row="0" x:Name="TopPanel" JournalOwnership="OwnsJournal" NavigationUIVisibility="Hidden" Visibility="Hidden"/>
        <Frame Grid.Row="1" x:Name="Frame" JournalOwnership="OwnsJournal" NavigationUIVisibility="Hidden" />
    </Grid>
</controls:MetroWindow>
    